<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>ArticleHyperlink__c</fullName>
    <description>Hyperlink to article with article title as label.</description>
    <externalId>false</externalId>
    <formula>HYPERLINK(URL__c, IF(ISBLANK(Site__c), Title__c, Title__c &amp; &apos; | &apos; &amp; Site__c))</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Article Hyperlink</label>
    <required>false</required>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
