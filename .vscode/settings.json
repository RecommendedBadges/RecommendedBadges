{
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/.sfdx": true
  },
  "salesforcedx-vscode-core.push-or-deploy-on-save.enabled": true,
  "salesforcedx-vscode-core.show-cli-success-msg": false,
  "apexPMD.rulesets": [
    ".custompmdrules.xml"
  ],
  "salesforcedx-vscode-core.push-or-deploy-on-save.preferDeployOnSave": false,
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    ".mule": true
  },
  "circleci.filters.branchFilter": "currentBranch",
  "codeAnalyzer.pMD.customConfigFile": ".custompmdrules.xml",
  "codeAnalyzer.analyzeOnOpen.enabled": false
}
